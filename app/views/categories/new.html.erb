<%= render 'shared/content_title', title: 'New Category' %>

<%= render 'shared/form_error_alerts', obj: @category %>

<div class="well">
  <%= form_for @category do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.submit "Create Category", class: "btn btn-success" %>
  <% end %>
</div>