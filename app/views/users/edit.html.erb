<%= render 'shared/content_title', title: 'Edit Profile' %>

<div class="well">
  <%= form_for @user do |f| %>
    <%= render 'shared/form_error_alerts', obj: @user %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.submit 'Update', class: 'btn btn-success' %>
  <% end %>
</div>